{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eddy-prac1"
		},
		"ls_ablobs_eddystrorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablobs_eddystrorage1'"
		},
		"ls_adls_eddystoragedatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_eddystoragedatalake'"
		},
		"ls_aztraining_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_aztraining_sa'"
		},
		"ls_adls_eddystoragedatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eddystoragedatalake.dfs.core.windows.net/"
		},
		"ls_http_cases_deaths_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/cloudboxacademy/covid19/blob/main/raw/main/ecdc_data/cases_deaths.csv"
		},
		"tr_get_populaption_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f4f2d56a-35c6-453f-a783-504ef6cbf3e1/resourceGroups/eddy-arch/providers/Microsoft.Storage/storageAccounts/eddystrorage1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "eddy-prac1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablobs_eddystrorage1_connectionString": {
						"value": "[parameters('ls_ablobs_eddystrorage1_connectionString')]"
					},
					"ls_adls_eddystoragedatalake_accountKey": {
						"value": "[parameters('ls_adls_eddystoragedatalake_accountKey')]"
					},
					"ls_aztraining_sa_connectionString": {
						"value": "[parameters('ls_aztraining_sa_connectionString')]"
					},
					"ls_adls_eddystoragedatalake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_eddystoragedatalake_properties_typeProperties_url')]"
					},
					"ls_http_cases_deaths_properties_typeProperties_url": {
						"value": "[parameters('ls_http_cases_deaths_properties_typeProperties_url')]"
					},
					"tr_get_populaption_properties_typeProperties_scope": {
						"value": "[parameters('tr_get_populaption_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}